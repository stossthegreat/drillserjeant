{
  "name": "drillsergeant-api",
  "version": "0.1.0",
  "description": "DrillSergeant Backend API",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:worker": "ts-node --transpile-only scripts/worker.ts",
    "start:worker:dist": "node dist/scripts/worker.js",
    "gen:openapi": "node -e \"console.log('OpenAPI generated in main.ts bootstrap')\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy || prisma migrate dev --name init"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-fastify": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/schedule": "^3.0.3",
    "bullmq": "^5.13.0",
    "ioredis": "^5.4.1",
    "@prisma/client": "^5.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^4.25.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "openai": "^4.57.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2"
  }
}
